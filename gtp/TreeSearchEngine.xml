<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="Bot1" class="tesuji.games.go.engine.TreeSearchEngine" singleton="false">
  		<property name="jar" value="TreeSearchEngine.jar"/>
  		<constructor-arg>
			<bean class="tesuji.games.go.search.MonteCarloTreeSearch" singleton="false">
				<property name="monteCarloAdministration">
					<bean class="tesuji.games.go.monte_carlo.MCTacticsAdministration" singleton="false">
				  		<property name="USE_HARD_PATTERNS">
				  			<value>false</value>
				  		</property>
				  		<property name="USE_TACTICS_IN_EXPLORATION">
				  			<value>true</value>
				  		</property>
				  	</bean>
				</property>
		  		<property name="secondsPerMove">
		  			<value>0</value>
		  		</property>
		  		<property name="minimumNrNodes">
		  			<value>2000</value>
		  		</property>
		  		<property name="nrProcessors">
		  			<value>1</value>
		  		</property>
		  		<property name="useAMAF">
		  			<value>true</value>
		  		</property>
		  		<property name="nrSimulationsBeforeExpansion">
		  			<value>1</value>
		  		</property>
		  		<property name="isTestVersion">
		  			<value>false</value>
		  		</property>
			</bean>
  		</constructor-arg>
  		<property name="isTestVersion">
  			<value>false</value>
  		</property>
  	</bean>

	<bean id="Bot2" class="tesuji.games.go.engine.TreeSearchEngine" singleton="false">
  		<property name="jar" value="TreeSearchEngine.jar"/>
  		<constructor-arg>
			<bean class="tesuji.games.go.search.MonteCarloTreeSearchWithPatterns" singleton="false">
				<property name="monteCarloAdministration">
					<bean class="tesuji.games.go.monte_carlo.MCPatternsAdministration" singleton="false">
  						<constructor-arg>
  							<value>GenTest</value>
  						</constructor-arg>
				  		<property name="USE_HARD_PATTERNS">
				  			<value>false</value>
				  		</property>
				  		<property name="USE_TACTICS_IN_EXPLORATION">
				  			<value>true</value>
				  		</property>
				  	</bean>
				</property>
		  		<property name="secondsPerMove">
		  			<value>0</value>
		  		</property>
		  		<property name="nrProcessors">
		  			<value>1</value>
		  		</property>
		  		<property name="minimumNrNodes">
		  			<value>2000</value>
		  		</property>
		  		<property name="useAMAF">
		  			<value>true</value>
		  		</property>
		  		<property name="nrSimulationsBeforeExpansion">
		  			<value>1</value>
		  		</property>
		  		<property name="isTestVersion">
		  			<value>false</value>
		  		</property>
			</bean>
  		</constructor-arg>
  		<property name="isTestVersion">
  			<value>false</value>
  		</property>
  	</bean>

	<bean id="eyeMoveFilter" class="tesuji.games.go.monte_carlo.EyeMoveFilter" singleton="false"/>

	<bean id="Bot3" class="tesuji.games.go.engine.TreeSearchEngine" singleton="false">
  		<property name="jar" value="TreeSearchEngine.jar"/>
  		<constructor-arg>
			<bean class="tesuji.games.go.search.MonteCarloTreeSearch" singleton="false">
				<property name="monteCarloAdministration">
					<bean class="tesuji.games.go.monte_carlo.MonteCarloPluginAdministration" singleton="false">
						<property name="simulationMoveFilterList">
							<list>
								<ref bean="eyeMoveFilter"/>
							</list>
						</property>
						<property name="explorationMoveFilterList">
							<list>
								<ref bean="eyeMoveFilter"/>
							</list>
						</property>
						<property name="simulationMoveGeneratorList">
							<list>
								<bean class="tesuji.games.go.monte_carlo.ImmediateAtariEscape" singleton="false"/>
								<bean class="tesuji.games.go.monte_carlo.ImmediateCapture" singleton="false"/>
								<bean class="tesuji.games.go.monte_carlo.ImmediateLadder" singleton="false"/>
							</list>
						</property>
						<property name="explorationMoveGeneratorList">
							<list>
								<bean class="tesuji.games.go.monte_carlo.ImmediateAtariEscape" singleton="false"><property name="urgency"><value>5</value></property></bean>
								<bean class="tesuji.games.go.monte_carlo.ImmediateCapture" singleton="false"><property name="urgency"><value>5</value></property></bean>
								<bean class="tesuji.games.go.monte_carlo.ImmediateLadder" singleton="false"><property name="urgency"><value>5</value></property></bean>
							</list>
						</property>
				  	</bean>
				</property>
		  		<property name="secondsPerMove">
		  			<value>0</value>
		  		</property>
		  		<property name="nrProcessors">
		  			<value>1</value>
		  		</property>
		  		<property name="minimumNrNodes">
		  			<value>2000</value>
		  		</property>
		  		<property name="useAMAF">
		  			<value>true</value>
		  		</property>
		  		<property name="nrSimulationsBeforeExpansion">
		  			<value>1</value>
		  		</property>
		  		<property name="isTestVersion">
		  			<value>false</value>
		  		</property>
			</bean>
  		</constructor-arg>
  		<property name="isTestVersion">
  			<value>false</value>
  		</property>
  	</bean>

	<bean id="Bot4" class="tesuji.games.go.engine.TreeSearchEngine" singleton="false">
  		<property name="jar" value="TreeSearchEngine.jar"/>
  		<constructor-arg>
			<bean class="tesuji.games.go.search.MonteCarloTreeSearch" singleton="false">
				<property name="monteCarloAdministration">
					<bean class="tesuji.games.go.monte_carlo.MCTacticsAdministration" singleton="false">
				  		<property name="USE_TACTICS_IN_EXPLORATION">
				  			<value>false</value>
				  		</property>
				  	</bean>
				</property>
		  		<property name="secondsPerMove">
		  			<value>0</value>
		  		</property>
		  		<property name="nrProcessors">
		  			<value>1</value>
		  		</property>
		  		<property name="minimumNrNodes">
		  			<value>2000</value>
		  		</property>
		  		<property name="useAMAF">
		  			<value>true</value>
		  		</property>
		  		<property name="nrSimulationsBeforeExpansion">
		  			<value>1</value>
		  		</property>
		  		<property name="isTestVersion">
		  			<value>false</value>
		  		</property>
			</bean>
  		</constructor-arg>
  		<property name="isTestVersion">
  			<value>false</value>
  		</property>
  	</bean>

	<bean id="RefBot" class="tesuji.games.go.engine.TreeSearchEngine" singleton="false">
  		<property name="jar" value="TreeSearchEngine.jar"/>
  		<constructor-arg>
			<bean class="tesuji.games.go.search.MonteCarloTreeSearch" singleton="false">
				<property name="monteCarloAdministration">
					<bean class="tesuji.games.go.monte_carlo.MonteCarloGoAdministration" singleton="false">
				  	</bean>
				</property>
		  		<property name="secondsPerMove">
		  			<value>0</value>
		  		</property>
		  		<property name="nrProcessors">
		  			<value>1</value>
		  		</property>
		  		<property name="minimumNrNodes">
		  			<value>2000</value>
		  		</property>
		  		<property name="useAMAF">
		  			<value>true</value>
		  		</property>
		  		<property name="nrSimulationsBeforeExpansion">
		  			<value>1</value>
		  		</property>
		  		<property name="isTestVersion">
		  			<value>false</value>
		  		</property>
			</bean>
  		</constructor-arg>
  	</bean> 
  	
	<bean id="RefBot1" class="tesuji.games.go.engine.RefBotEngine" singleton="false">
  		<property name="jar" value="TesujiRefBot.jar"/>
  		<property name="nrSimulations">
  			<value>2000</value>
  		</property>
		<property name="monteCarloAdministration">
					<bean class="tesuji.games.go.monte_carlo.MonteCarloPluginAdministration" singleton="false">
						<property name="simulationMoveFilterList">
							<list>
								<ref bean="eyeMoveFilter"/>
							</list>
						</property>
						<property name="explorationMoveFilterList">
							<list>
								<ref bean="eyeMoveFilter"/>
							</list>
						</property>
				  	</bean>
		</property>
  		<property name="resignEnabled">
  			<value>true</value>
  		</property>
  	</bean> 
  	
	<bean id="RefBot2" class="tesuji.games.go.engine.RefBotEngine" singleton="false">
  		<property name="jar" value="TesujiRefBot.jar"/>
  		<property name="nrSimulations">
  			<value>2000</value>
  		</property>
		<property name="monteCarloAdministration">
					<bean class="tesuji.games.go.monte_carlo.MonteCarloGoAdministration" singleton="false">
				  	</bean>
		</property>
  		<property name="resignEnabled">
  			<value>true</value>
  		</property>
  	</bean> 
  	
	<bean id="RefBot3" class="tesuji.games.go.reference.engine.RefBotEngine" singleton="false">
  		<property name="jar" value="TesujiRefBot.jar"/>
  		<property name="nrSimulations">
  			<value>2000</value>
  		</property>
		<property name="monteCarloAdministration">
			<bean class="tesuji.games.go.reference.monte_carlo.MCLibertyAdministration" singleton="false"/>
		</property>
  		<property name="resignEnabled">
  			<value>true</value>
  		</property>
  	</bean> 

</beans>